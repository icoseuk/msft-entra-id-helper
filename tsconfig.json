{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "declaration": true,
      "sourceMap": true,
      "outDir": "./build",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src", ".eslintrc.cjs", "esbuild.js"],
  }